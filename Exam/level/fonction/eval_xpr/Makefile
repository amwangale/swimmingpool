variable1=  eval_expr.c ft_tools.c main.c

variable2 = -Wall -Wextra -Werror

all: 

$(NAME):
	 -o eval_expr $(variable2) $(SRC)

clean:
	rm -f *.o

removebinary: clean
	rm -f $(NAME)

re: fclean all
